{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/manifest.webmanifest", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**", "/*.(svg|cur|jpg|png|webp|gif|woff|woff2)"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "oauth-config",
      "urls": ["https://localhost:44396/.well-known/openid-configuration"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 1,
        "maxAge": "7d",
        "timeout": "3s"
      }
    },
    {
      "name": "abp-config",
      "urls": [
        "https://localhost:44396/api/abp/application-configuration",
        "https://localhost:44396/api/abp/application-localization"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 10,
        "maxAge": "1d",
        "timeout": "5s"
      }
    },
    {
      "name": "api-general",
      "urls": ["https://localhost:44396/api/**"],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "1h",
        "timeout": "10s"
      }
    }
  ]
}
